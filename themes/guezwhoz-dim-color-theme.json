{
	"name": "Guezwhoz (Dimmid)",
	"type": "dark",
	"tokenColors": [
		{
			"name": "Constants (Group 1)",
			"scope": [
				"support.function.builtin",
				"source.js constant.other.object.key.js string.unquoted.label.js",
				"punctuation.separator.dictionary.key-value",
				"punctuation.separator.key-value",
				"punctuation.definition.template-expression",
				"punctuation.definition.variable",
				"keyword",
				"constant.language",
				"variable.language",
				"keyword.control",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#87afd7"
			}
		},
		{
			"name": "Keyword, Storage (Group 1)",
			"scope": [
				"storage",
				"storage.modifier",
				"punctuation.section.embedded",
			],
			"settings": {
				"foreground": "#87afd7"
			}
		},
		{
			"name": "Tag, (Group 1)",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#87afd7"
			}
		},
		{
			"name": "Markup - Underline (Group 1)",
			"scope": [
				"markup.underline"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#87afd7"
			}
		},
		{
			"name": "Markdown - Link Anchor (Group 1)",
			"scope": [
				"constant.other.reference.link.markdown"
			],
			"settings": {
				"foreground": "#87afd7"
			}
		},
		{
			"name": "Variables, Object (Group 2)",
			"scope": [
				"variable",
				"variable.parameter",
				"support.constant",
				"support.constant.media-type",
				"support.constant.media",
				"string constant.other.placeholder",
				"meta.object-literal.key",
				"entity.other.attribute-name",
				"source.json meta.structure.dictionary.json support.type.property-name.json",
			],
			"settings": {
				"foreground": "#87d7d7"
			}
		},
		{
			"name": "CSS Class and Support (Group 2)",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name",
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#87d7d7"
			}
		},
		{
			"name": "Markdown - Link (Group 2)",
			"scope": [
				"string.other.link.title.markdown"
			],
			"settings": {
				"foreground": "#87d7d7"
			}
		},
		{
			"name": "Markdown - Link  (Group 2)",
			"scope": [
				"string.other.link.title.markdown"
			],
			"settings": {
				"foreground": "#87d7d7"
			}
		},
		{
			"name": "Class (Group 3)",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.orther.namespace.use.php",
				"support.variable",
				"support.other.variable",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types",
				"entity.other.inherited-class",
				"entity.other.attribute-name.class",
			],
			"settings": {
				"foreground": "#87d7af"
			}
		},
		{
			"name": "Markdown - Heading (Group 3)",
			"scope": [
				"markdown.heading",
				"markup.heading | markup.heading entity.name",
				"markup.heading.markdown punctuation.definition.heading.markdown"
			],
			"settings": {
				"foreground": "#afd7af"
			}
		},
		{
			"name": "Markup - Italic (Group 3)",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#afd7af"
			}
		},
		{
			"name": "Markup - Bold (Group 3)",
			"scope": [
				"markup.bold",
				"markup.bold string"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#afd7af"
			}
		},
		{
			"name": "Markup - Bold-Italic (Group 3)",
			"scope": [
				"markup.bold markup.italic",
				"markup.italic markup.bold",
				"markup.quote markup.bold",
				"markup.bold markup.italic string",
				"markup.italic markup.bold string",
				"markup.quote markup.bold string"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#afd7af"
			}
		},
		{
			"name": "Number, (Group 4)",
			"scope": [
				"constant.numeric",
				"keyword.other.unit",
				"support.variable.property.js"
			],
			"settings": {
				"foreground": "#d7d7af"
			}
		},
		{
			"name": "Regular Expressions definition (Group 4)",
			"scope": [
				"constant.character.character-class.regexp",
				"constant.other.character-class.set.regexp",
				"constant.other.character-class.regexp",
				"constant.character.set.regexp",
				"punctuation.definition.group.regexp",
				"punctuation.definition.group.assertion.regexp",
				"punctuation.definition.character-class.regexp",
				"punctuation.character.set.begin.regexp",
				"punctuation.character.set.end.regexp",
				"keyword.operator.negation.regexp",
				"support.other.parenthesis.regexp"
			],
			"settings": {
				"foreground": "#d7d7af"
			}
		},
		{
			"name": "Function, Special Method (Group 5)",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method",
				"support.constant.handlebars",
				"source.powershell variable.other.member",
				"entity.name.operator.custom-literal",
				"entity.other.attribute-name.pseudo-class",
				"string.interpolated.dollar.shell"
			],
			"settings": {
				"foreground": "#afafd7"
			}
		},
		{
			"name": "punctuation dim, Comments (Group 6)",
			"scope": [
				"punctuation.definition.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"comment",
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "Markdown - Link Description (Group 6)",
			"scope": [
				"string.other.link.description.title.markdown"
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "Punctuation (Group 7)",
			"scope": [
				"punctuation",
				"meta.property-list",
				"meta.brace.round.ts"
			],
			"settings": {
				"foreground": "#9e9e9e" // DONE COR FINAL
			}
		},
		{
			"name": "String (Group 8)",
			"scope": [
				"string",
				"meta.tag",
				"support.constant.property-value",
				"support.constant.font-name",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color",
				"keyword.operator.ternary",
				"keyword.operator.relational",
				"keyword.operator.assignment",
				"keyword.operator.comparison",
				"keyword.operator.logical",
				"keyword.operator.arithmetic",
				"text.html.markdown",
			],
			"settings": {
				"foreground": "#dadada"
			}
		},
		{
			"name": "Regular Expressions (Group 8)",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#dadada"
			}
		},
		{
			"name": "Escape Characters (Group Special)",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#5fffaf"
			}
		},
		{
			"name": "Invalid (Group Special)",
			"scope": [
				"invalid",
				"invalid.illegal",
				"markup.deleted"
			],
			"settings": {
				"foreground": "#d75f5f"
			}
		},
		{
			"name": "URL (Group Special)",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup - Quote",
			"scope": [
				"markup.quote"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
	],
	"colors": {
