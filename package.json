{
  "name": "guezwhoz-vscode-theme",
  "displayName": "Guezwhoz Theme",
  "description": "Dark vscode theme based on cool palette.",
  "publisher": "guezwhoz-schema",
  "version": "1.2.0",
  "license": "MIT",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1c1c1c",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guesswhozzz/guezwhoz-vscode-theme.git"
  },
  "keywords": [
    "Theme",
    "Dark Theme"
  ],
  "contributes": {
    "themes": [
      {
        "id": "Guezwhoz",
        "label": "Guezwhoz (Main)",
        "uiTheme": "vs-dark",
        "path": "./themes/guezwhoz-base-color-theme.json"
      }
    ],
    "iconThemes": [
      {
        "id": "Guezwhoz (icons-set)",
        "label": "Guezwhoz (icons)",
        "path": "./fileicons/guezwhoz-icon-theme.json"
      }
    ],
    "grammars": [
      {
        "path": "./syntaxes/injection.json",
        "scopeName": "todo-comment.injection",
        "injectTo": [
          "source.js",
          "source.jsx",
          "source.ts",
          "source.tsx",
          "source.html",
          "source.php",
          "source.css",
          "source.scss",
          "source.c"
        ]
      }
    ]
  },
  "bugs": {
    "url": "https://github.com/guesswhozzz/guezwhoz-vscode-theme/issues",
    "email": "guezwhoz@gmail.com"
  },
  "homepage": "https://github.com/guesswhozzz/guezwhoz-vscode-theme/blob/master/README.md"
}